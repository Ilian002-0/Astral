{
  "short_name": "Atlas",
  "name": "Atlas Trading Dashboard",
  "description": "Atlas helps you visualize your trading history from a CSV file. Upload your trade report to see an interactive dashboard with key performance metrics and a beautiful equity curve chart.",
  "icons": [
    {
      "src": "https://i.imgur.com/TN8saNO.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "https://i.imgur.com/TN8saNO.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/logo-with-bg.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "start_url": "/",
  "id": "/",
  "display": "standalone",
  "orientation": "any",
  "theme_color": "#0c0b1e",
  "background_color": "#0c0b1e",
  "display_override": ["window-controls-overlay"],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "shortcuts": [
    {
      "name": "Dashboard",
      "url": "/?view=dashboard",
      "icons": [
        { "src": "/dashboard-icon.svg", "sizes": "any", "type": "image/svg+xml" }
      ]
    },
    {
      "name": "List of Trades",
      "url": "/?view=trades",
      "icons": [
        { "src": "/list-icon.svg", "sizes": "any", "type": "image/svg+xml" }
      ]
    },
    {
      "name": "Calendar",
      "url": "/?view=calendar",
      "icons": [
        { "src": "/calendar-icon.svg", "sizes": "any", "type": "image/svg+xml" }
      ]
    },
    {
      "name": "Goals",
      "url": "/?view=goals",
      "icons": [
        { "src": "/goals-icon.svg", "sizes": "any", "type": "image/svg+xml" }
      ]
    }
  ],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "text/csv": [".csv"]
      },
      "launch_type": "single-client"
    }
  ]
}